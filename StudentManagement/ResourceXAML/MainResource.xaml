                    xmlns:viewmodel_Summary="clr-namespace:StudentManagement.ViewModel.Summary_Manage_ViewModel">
    <viewmodel:MainViewModel x:Key="MainVM"></viewmodel:MainViewModel>
    <viewmodel_Login:Login_WD_ViewModel x:Key="Login_WD_VM"></viewmodel_Login:Login_WD_ViewModel>
    
